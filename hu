import discord
from discord.ext import commands

bot = commands.Bot(command_prefix="!")

@bot.command()
async def show_embed(ctx):
    # Image URL
    image_url = "https://cdn.discordapp.com/attachments/1464288520267894785/1470928587015323688/drivemecrazy16-1.jpg"

    # Repeat the image four times in separate embeds
    for _ in range(4):
        embed = discord.Embed(
            title="drivemecrazy16-1.jpg",  # From your first screenshot
            color=discord.Color.blue()     # Match the original color if you want
        )
        embed.set_author(
            name="drivemecrazy16",
            icon_url="https://cdn.discordapp.com/avatars/1464288520267894785/someicon.png"  # Replace with the icon from your screenshot
        )
        embed.set_image(url=image_url)
        embed.set_footer(text="Sent via Discord Bot")
        embed.timestamp = ctx.message.created_at  # Use current timestamp

        await ctx.send(embed=embed)

bot.run("YOUR_BOT_TOKEN")